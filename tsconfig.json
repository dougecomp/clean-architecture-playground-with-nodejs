{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    // "rootDir": "src",
    // "baseUrl": "src",
    // "paths": {},
    // "rootDirs": [],
    // "typeRoots": [],
    "types": ["vitest/globals"],
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "src/**/*"
  ]
}
